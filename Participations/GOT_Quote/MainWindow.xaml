<Window x:Class="GOT_Quote.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GOT_Quote"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/got.jpg"/>
        </Grid.Background>
        <Button x:Name="btnGetQuote" Content="Get Quote" HorizontalAlignment="Left" Height="109" Margin="25,27,0,0" VerticalAlignment="Top" Width="221" Click="btnGetQuote_Click"/>
        <TextBox x:Name="txtQuote" HorizontalAlignment="Left" Height="112" Margin="97,259,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="587"/>

    </Grid>
</Window>
